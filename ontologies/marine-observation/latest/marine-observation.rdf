        <rdfs:isDefinedBy rdf:resource="https://eurobis.org/ns/marine-observation"/>
        <rdfs:label xml:lang="it">produce</rdfs:label>
        <rdfs:label xml:lang="nl">produceert</rdfs:label>
        <rdfs:label xml:lang="en">produces</rdfs:label>
    </owl:ObjectProperty>



