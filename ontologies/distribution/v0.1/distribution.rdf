        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/distribution"/>
        <rdfs:label xml:lang="en">Invasiveness</rdfs:label>
        <rdfs:label xml:lang="it">Invasività</rdfs:label>
