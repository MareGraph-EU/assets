     -->

    


    <!-- https://w3id.org/marine/ns/taxa-name/aphiaID -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/marine/ns/taxa-name/aphiaID">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Codice univoco e persistente nel sistema WoRMS.</rdfs:comment>
        <rdfs:comment xml:lang="en">Unique and persistent code within WoRMS.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="en">aphia identifier</rdfs:label>
        <rdfs:label xml:lang="it">identificativo aphia</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/authorship -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/marine/ns/taxa-name/authorship">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:comment xml:lang="it">La combinazione di autore e anno associata al nome di un taxon, espressa come stringa.</rdfs:comment>
        <rdfs:comment xml:lang="en">The combination of author and year associated with a taxon name, expressed as a string.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="en">authorship</rdfs:label>
        <rdfs:label xml:lang="it">paternità</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/fullScientificName -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/marine/ns/taxa-name/fullScientificName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2008/05/skos-xl#literalForm"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:comment xml:lang="it">Il nome scientifico completo in latino che include anche la paternità, data dalla combinazione di autore e anno.</rdfs:comment>
        <rdfs:comment xml:lang="en">The complete scientific name in Latin including the authorship information (author, year).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="en">full scientific name (with author)</rdfs:label>
        <rdfs:label xml:lang="it">nome scientifico completo (con autore)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/scientificName -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/marine/ns/taxa-name/scientificName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2008/05/skos-xl#literalForm"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:comment xml:lang="it">Il nome scientifico in latino che non include le informazioni sulla paternità (autore, anno)</rdfs:comment>
        <rdfs:comment xml:lang="en">The scientific name in Latin that does not include information on the authorship (author, year).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">nome scientifico (senza autore)</rdfs:label>
        <rdfs:label xml:lang="en">scientific name (without author)</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/vernacularName -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/marine/ns/taxa-name/vernacularName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2008/05/skos-xl#literalForm"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/VernacularName"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
        <rdfs:comment xml:lang="it">Il nome comune espresso nelle varie lingue</rdfs:comment>
        <rdfs:comment xml:lang="en">The common name expressed in various languages.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">nome comune</rdfs:label>
        <rdfs:label xml:lang="en">vernacular name</rdfs:label>
    </owl:DatatypeProperty>
    


