<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/marine/ns/taxa-name/"
     xml:base="https://w3id.org/marine/ns/taxa-name/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="https://w3id.org/marine/ns/taxa-name">
        <owl:versionIRI rdf:resource="https://w3id.org/marine/ns/taxa-name/0.1"/>
        <owl:imports rdf:resource="http://www.w3.org/2008/05/skos-xl"/>
        <owl:imports rdf:resource="https://w3id.org/marine/ns/top-level/0.1"/>
        <dct:creator rdf:resource="https://w3id.org/italia/data/organization/support-unit/cnr-Z6HZEH/stlab"/>
        <dct:creator rdf:resource="https://w3id.org/marine/resource/organisation/dv"/>
        <dct:creator rdf:resource="https://w3id.org/marine/resource/organisation/vliz"/>
        <dct:description xml:lang="it">Questa è l&apos;ontologia dei nomi dei taxon e di tutte le informazione tassonomiche come disponibili dal sistema WoRMS. Essa importa l&apos;ontologia di primo livello MAREGRAPH ed è allineata a skos-xl.</dct:description>
        <dct:description xml:lang="en">This is the ontology of taxon names and all taxonomic information as available from the WoRMS system. It imports the MAREGRAPH top-level ontology and is aligned to skos-xl.</dct:description>
        <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-08-30</dct:issued>
        <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/>
        <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ITA"/>
        <dct:license rdf:resource="http://publications.europa.eu/resource/authority/licence/CC_BY_4_0"/>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-11-30</dct:modified>
        <dct:title xml:lang="it">MAREGRAPH - Ontologia dei nomi dei taxon</dct:title>
        <dct:title xml:lang="en">MAREGRAPH - Taxa names ontology</dct:title>
        <rdfs:comment xml:lang="it">Questa è l&apos;ontologia dei nomi dei taxon e di tutte le informazione tassonomiche come disponibili dal sistema WoRMS. Essa importa l&apos;ontologia di primo livello MAREGRAPH ed è allineata a skos-xl.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the ontology of taxon names and all taxonomic information as available from the WoRMS system. It imports the MAREGRAPH top-level ontology and is aligned to skos-xl.</rdfs:comment>
        <rdfs:label xml:lang="it">MAREGRAPH - Ontologia dei nomi dei taxon</rdfs:label>
        <rdfs:label xml:lang="en">MAREGRAPH - Taxa names ontology</rdfs:label>
        <owl:versionInfo xml:lang="en">Preliminary version 0.1</owl:versionInfo>
        <owl:versionInfo xml:lang="it">Versione preliminare 0.1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/language -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/language"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://www.w3.org/2002/07/owl#minQualifiedCardinality -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#minQualifiedCardinality"/>
    


    <!-- http://www.w3.org/ns/prov#wasDerivedFrom -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/ns/prov#wasDerivedFrom"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/coverage -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/coverage"/>
    


    <!-- https://w3id.org/marine/ns/taxa-name/denotes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/denotes">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/marine/ns/taxa-name/isDenotedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/top-level/Name"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/Taxon"/>
        <rdfs:comment xml:lang="en">Property used to represent a name for a taxon.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per definire un nome per un taxon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">definisce/denota</rdfs:label>
        <rdfs:label xml:lang="en">denotes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/hasAcceptedName -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/hasAcceptedName">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/marine/ns/taxa-name/isDenotedBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/Taxon"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:comment xml:lang="en">Property used to represent the valid/accepted name for a taxon.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per definire il nome valido/accettato per un taxon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">ha nome valido/accettato</rdfs:label>
        <rdfs:label xml:lang="en">has accepted name</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/hasAuthorship -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/hasAuthorship">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/Authorship"/>
        <rdfs:comment xml:lang="en">Property used to represent the situation of authorship, given by the combination of the author and year, for a taxon name.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per definire la situazione di paternità, data dalla combinazione di autore e anno, per un nome di taxon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">ha paternità</rdfs:label>
        <rdfs:label xml:lang="en">has authorship</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/hasChild -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/hasChild">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#narrower"/>
        <owl:inverseOf rdf:resource="https://w3id.org/marine/ns/taxa-name/hasParent"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/Taxon"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/Taxon"/>
        <rdfs:comment xml:lang="en">Property that allows representing the direct child taxon in a classification.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di rappresentare il taxon diretto inferiore in una classification.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">ha taxon inferiore</rdfs:label>
        <rdfs:label xml:lang="en">has child taxon</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/hasParent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/hasParent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#broader"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/Taxon"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/Taxon"/>
        <rdfs:comment xml:lang="en">Property that allows representing the direct parent taxon in a classification. The parent of a species is usually a genus; the parent of a genus is usually a family, and so on.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di rappresentare il taxon diretto superiore in una classificatione. Il taxon diretto superiore di una specie è di solito il genere, quello del genere è la famiglia e così via.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">ha taxon superiore</rdfs:label>
        <rdfs:label xml:lang="en">has parent taxon</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/hasStatus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/hasStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/Status"/>
        <rdfs:comment xml:lang="en">Property used to connect the taxon name with its status (e.g., accepted, unaccepted).</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per collegare il nome del taxon con il suo stato (e.g., accettato, non accettato).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">ha stato</rdfs:label>
        <rdfs:label xml:lang="en">has status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/hasSynonym -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/hasSynonym">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/relation"/>
        <owl:inverseOf rdf:resource="https://w3id.org/marine/ns/taxa-name/isSynonymOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:comment xml:lang="en">Property used to specify synonymity relations between taxon names.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per specificare relazioni di sinonimia tra nomi di taxon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">ha sinonimo</rdfs:label>
        <rdfs:label xml:lang="en">has synonym</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/hasSynonymName -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/hasSynonymName">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/marine/ns/taxa-name/isDenotedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/Taxon"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:comment xml:lang="en">Proprietà used to connect the taxon with its names considered synonyms.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per collegare il taxon con i suoi nomi considerati sinonimi.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">ha nome sinonimo</rdfs:label>
        <rdfs:label xml:lang="en">has synonym name</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/hasUnacceptedReason -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/hasUnacceptedReason">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/UnacceptedReason"/>
        <rdfs:comment xml:lang="en">Property used to specify the motivation for which a name is not accepted for a taxon name.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per specificare il motivo per cui un nome non è accettato per un nome di taxon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="it">ha motivo non accettato</rdfs:label>
        <rdfs:label xml:lang="en">ha unaccepted reason</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/isDenotedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/isDenotedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/Taxon"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/top-level/Name"/>
        <rdfs:comment xml:lang="en">Inverse property of &apos;denotes&apos; that allows one to specify the name for a taxon.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &apos;definito/denotato&apos; che consente di specificare il nome per un taxon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="en">is denoted by</rdfs:label>
        <rdfs:label xml:lang="it">è definito/denotato da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/isOfRank -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/isOfRank">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/Rank"/>
        <rdfs:comment xml:lang="en">Property used to specify the rank in a taxonomic classification for the taxon name.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per specificare il rango in una classificazione tassonomica per un nome di taxon.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="en">is of rank</rdfs:label>
        <rdfs:label xml:lang="it">è di rango</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/marine/ns/taxa-name/isSynonymOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/marine/ns/taxa-name/isSynonymOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/relation"/>
        <rdfs:domain rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:range rdf:resource="https://w3id.org/marine/ns/taxa-name/TaxonName"/>
        <rdfs:comment xml:lang="en">Inverse property of &apos;has synonym&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &apos;ha sinonimo&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/marine/ns/taxa-name"/>
        <rdfs:label xml:lang="en">is synonym of</rdfs:label>
        <rdfs:label xml:lang="it">è sinonimo di</rdfs:label>
    </owl:ObjectProperty>
    


