    <!-- http://purl.org/dc/terms/LinguisticSystem -->

    <owl:Class rdf:about="http://purl.org/dc/terms/LinguisticSystem"/>
    


    <!-- http://purl.org/dc/terms/Location -->

    <owl:Class rdf:about="http://purl.org/dc/terms/Location"/>
    


    <!-- https://w3id.org/marine/ns/taxa-name/Authorship -->

    <owl:Class rdf:about="https://w3id.org/marine/ns/taxa-name/Authorship">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/marine/ns/top-level/hasAuthor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
