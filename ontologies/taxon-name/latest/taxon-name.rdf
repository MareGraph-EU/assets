        <rdfs:label xml:lang="nl">aphia identificatie</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://aphia.org/ns/taxon-name/authorship -->

    <owl:DatatypeProperty rdf:about="https://aphia.org/ns/taxon-name/authorship">
        <rdfs:subPropertyOf rdf:resource="http://rs.tdwg.org/dwc/terms/scientificNameAuthorship"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
