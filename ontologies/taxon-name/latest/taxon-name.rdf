        <rdfs:subPropertyOf rdf:resource="http://rs.tdwg.org/dwc/terms/scientificNameID"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
