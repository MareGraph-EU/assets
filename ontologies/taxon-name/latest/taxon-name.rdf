    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
    


    <!-- https://aphia.org/ns/taxon-name/denotes -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/denotes">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <owl:inverseOf rdf:resource="https://aphia.org/ns/taxon-name/isDenotedBy"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/top-level/Name"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/Taxon"/>
        <rdfs:comment xml:lang="en">Property used to represent a name for a taxon.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per definire un nome per un taxon.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap die wordt gebruikt om een naam voor een taxon te definiëren.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="it">definisce/denota</rdfs:label>
        <rdfs:label xml:lang="en">denotes</rdfs:label>
        <rdfs:label xml:lang="nl">definieert</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/hasAcceptedName -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/hasAcceptedName">
        <rdfs:subPropertyOf rdf:resource="https://aphia.org/ns/taxon-name/isDenotedBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/Taxon"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:comment xml:lang="en">Property used to represent the currently valid (zoological) or accepted (botanical) scientific name for a taxon.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per rappresentare il nome scientifico attualmente valido (zoologico) o accettato (botanico) di un taxon.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap gebruikt om de huidige geldige (zoölogische) of aanvaarde (botanische) wetenschappelijke naam voor een taxon weer te geven.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="it">ha nome valido/accettato</rdfs:label>
        <rdfs:label xml:lang="en">has accepted name</rdfs:label>
        <rdfs:label xml:lang="nl">heeft geaccepteerde naam</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/hasChild -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/hasChild">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#narrower"/>
        <owl:inverseOf rdf:resource="https://aphia.org/ns/taxon-name/hasParent"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/Taxon"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/Taxon"/>
        <rdfs:comment xml:lang="en">Property that allows representing the direct child taxon in a classification.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di rappresentare il taxon diretto inferiore in una classification.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap die toelaat het directe lagere taxon in een classificatie weer te geven.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="it">ha taxon inferiore</rdfs:label>
        <rdfs:label xml:lang="en">has child taxon</rdfs:label>
        <rdfs:label xml:lang="nl">heeft lager taxon</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/hasParent -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/hasParent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#broader"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/Taxon"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/Taxon"/>
        <rdfs:comment xml:lang="en">Property that allows representing the direct parent taxon in a classification. The parent of a species is usually a genus; the parent of a genus is usually a family, and so on.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che consente di rappresentare il taxon diretto superiore in una classificatione. Il taxon diretto superiore di una specie è di solito il genere, quello del genere è la famiglia e così via.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap die toelaat het directe hogere taxon in een classificatie weer te geven. De ouder van een soort is meestal een geslacht; de ouder van een geslacht is meestal een familie, enzovoort.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="it">ha taxon superiore</rdfs:label>
        <rdfs:label xml:lang="en">has parent taxon</rdfs:label>
        <rdfs:label xml:lang="nl">heeft hoger taxon</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/hasStatus -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/hasStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/Status"/>
        <rdfs:comment xml:lang="en">Property used to connect the taxon name with its status (e.g., accepted, unaccepted).</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per collegare il nome del taxon con il suo stato (e.g., accettato, non accettato).</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap gebruikt om de taxonnaam te koppelen aan zijn status (bijv. geaccepteerd, niet geaccepteerd).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="it">ha stato</rdfs:label>
        <rdfs:label xml:lang="en">has status</rdfs:label>
        <rdfs:label xml:lang="nl">heeft status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/hasSynonym -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/hasSynonym">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/relation"/>
        <owl:inverseOf rdf:resource="https://aphia.org/ns/taxon-name/isSynonymOf"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:comment xml:lang="en">Property used to specify synonymity relations between taxon names.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per specificare relazioni di sinonimia tra nomi di taxon.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap gebruikt om synoniemenrelaties tussen taxonnamen te specificeren.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="it">ha sinonimo</rdfs:label>
        <rdfs:label xml:lang="en">has synonym</rdfs:label>
        <rdfs:label xml:lang="nl">heeft synoniem</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/hasSynonymName -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/hasSynonymName">
        <rdfs:subPropertyOf rdf:resource="https://aphia.org/ns/taxon-name/isDenotedBy"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/Taxon"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:comment xml:lang="en">Proprietà used to connect the taxon with its names considered synonyms.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per collegare il taxon con i suoi nomi considerati sinonimi.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap gebruikt om het taxon te koppelen aan zijn namen die als synoniemen worden beschouwd.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="it">ha nome sinonimo</rdfs:label>
        <rdfs:label xml:lang="en">has synonym name</rdfs:label>
        <rdfs:label xml:lang="nl">heeft synoniem naam</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/hasUnacceptedReason -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/hasUnacceptedReason">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/UnacceptedReason"/>
        <rdfs:comment xml:lang="en">Property used to specify the motivation for which a name is not accepted for a taxon name.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per specificare il motivo per cui un nome non è accettato per un nome di taxon.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap gebruikt om de motivatie te specificeren waarvoor een naam niet wordt geaccepteerd voor een taxonnaam.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="it">ha motivo non accettato</rdfs:label>
        <rdfs:label xml:lang="en">has unaccepted reason</rdfs:label>
        <rdfs:label xml:lang="nl">heeft niet-geaccepteerd reden</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/isDenotedBy -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/isDenotedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/Taxon"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/top-level/Name"/>
        <rdfs:comment xml:lang="en">Inverse property of &apos;denotes&apos; that allows one to specify the name for a taxon.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &apos;definito/denotato&apos; che consente di specificare il nome per un taxon.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Inverse eigenschap van 'definieert' die toelaat de naam van een taxon te definiëren.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="en">is denoted by</rdfs:label>
        <rdfs:label xml:lang="it">è definito/denotato da</rdfs:label>
        <rdfs:label xml:lang="nl">is gedefinieerd door</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/isOfRank -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/isOfRank">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/Rank"/>
        <rdfs:comment xml:lang="en">Property used to specify the rank in a taxonomic classification for the taxon name.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà utilizzata per specificare il rango in una classificazione tassonomica per un nome di taxon.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Eigenschap gebruikt om de taxonnaam te koppelen aan zijn rang in een taxonomische classificatie.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="en">is of rank</rdfs:label>
        <rdfs:label xml:lang="it">è di rango</rdfs:label>
        <rdfs:label xml:lang="nl">heeft rang</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/taxon-name/isSynonymOf -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/taxon-name/isSynonymOf">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/relation"/>
        <rdfs:domain rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:range rdf:resource="https://aphia.org/ns/taxon-name/TaxonName"/>
        <rdfs:comment xml:lang="en">Inverse property of &apos;has synonym&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &apos;ha sinonimo&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Inverse eigenschap van 'heeft synoniem'</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://aphia.org/ns/taxon-name"/>
        <rdfs:label xml:lang="en">is synonym of</rdfs:label>
        <rdfs:label xml:lang="it">è sinonimo di</rdfs:label>
        <rdfs:label xml:lang="nl">is synoniem van</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://aphia.org/ns/top-level/hasIdentifier -->

    <owl:ObjectProperty rdf:about="https://aphia.org/ns/top-level/hasIdentifier"/>
    


