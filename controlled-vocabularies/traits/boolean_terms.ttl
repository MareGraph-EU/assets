@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@prefix trait: <https://aphia.org/ns/vocab/attribute-trait/> .

trait:conceptscheme
    a skos:Conceptscheme ;
    rdfs:label "listing aphia attributes and traits"@en .

<https://aphia.org/ns/vocab/attribute-trait/>
    a skos:Collection ;
    rdfs:label "aphia attributes and traits"@en ;
    
    skos:member trait:brooding , trait:macroalgal_blooming ;
.

trait:brooding
    a skos:Collection ; 
    dct:description "..."@en ;

    skos:member trait:brooding_yes , trait:brooding_no ;
. 

trait:brooding_yes
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "Yes"@en ;
    skos:definition "..."@en ;
.

trait:brooding_no
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "No"@en ;
    skos:definition ""@en ;
.


trait:macroalgal_blooming
    a skos:Collection ; 
    dct:description "Types of blooming specific for macroalgae species."@en ;

    skos:member trait:macroalgal_blooming_yes , trait:macroalgal_blooming_no ;

    skos:closeMatch ... ; 
. 

trait:macroalgal_blooming_yes
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "Macroalgal blooming potential"@en ;
    skos:definition "The species has the potential to go through episodes of intense growth and mass proliferation of the thallus under specific environmental conditions such as high nutrient and temperature conditions."@en ;
.

trait:macroalgal_blooming_no
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "No macroalgal blooming potential"@en ;
    skos:definition "The species does not have the obvious potential to go through episodes of intense growth and mass proliferation of the thallus under specific environmental conditions such as high nutrient and temperature conditions."@en ;
.