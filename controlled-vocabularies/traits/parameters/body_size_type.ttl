@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@base <https://aphia.org/ns/traits/body_size_type> .

<>
    a skos:Collection ; 
    dct:description "The type of a body size (e.g. minimum, maximum, average)."@en ;

    skos:member <#minimum> , 
                <#maximum> , 
                <#diameter> , 
                <#average> , 
                <#unknown_body_size_type> , 
                <#mean> , 
                <#standard_mean> , 
                <#common> ;
.

<#minimum>
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme <https://aphia.org/ns/traits/> ;
    skos:prefLabel "Minimum body size"@en ;
    skos:definition "Minimum body size"@en ;
.

<#maximum>
