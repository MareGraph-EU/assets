                <#manca> , 
                <#cypris> , 
                <#hatchling> , 
                <#copepodite_I> , 
                <#copepodite_II> , 
                <#copepodite_III> , 
                <#copepodite_IV> , 
                <#copepodite_V> , 
                <#copepodite_VI> , 
                <#gametophyte> , 
                <#sporophyte> , 
                <#macrothallus> , 
                <#microthallus> , 
                <#neonate> , 
                <#1_year_old> , 
                <#foetus> , 
                <#podocyst> , 
                <#polygastric> , 
