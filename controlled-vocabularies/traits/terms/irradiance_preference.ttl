@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@base <https://aphia.org/ns/term/irradiance_preference> .

<>
    a skos:Collection ; 
    dct:description "..."@en ;

    skos:member <#photophilic> , <#sciaphilic> ;

    skos:closeMatch ... ; 
. 

<#photophilic>
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme <https://aphia.org/ns/vocab/attribute-trait/> ;
    skos:prefLabel "Photophilic"@en ;
    skos:definition "Growing or thriving in habitats with high light intensity."@en ;

    skos:closeMatch ... ; 
.

<#sciaphilic>
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme <https://aphia.org/ns/vocab/attribute-trait/> ;
    skos:prefLabel "Sciaphilic"@en ;
    skos:definition "Growing or thriving in shaded or low light intensity habitats."@en ;

    skos:closeMatch ... ; 
.