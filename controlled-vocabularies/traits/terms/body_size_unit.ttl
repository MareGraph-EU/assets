@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@base <https://aphia.org/ns/traits/body_size_unit> .

<>
    a skos:Collection ; 
    dct:description "The unit of a body size (e.g. m, cm, mm)."@en ;

    skos:member <#micrometer> , 
                <#millimeter> , 
                <#centimeter> , 
                <#meter> , 
                <#square_centimeter> , 
                <#square_decimeter> , 
                <#square_meter> , 
                <#cubic_centimeter> , 
                <#inch> , 
                <#kilogram> , 
                <#ton> ; 
.

<#micrometer>
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme <https://aphia.org/ns/traits/> ;
    skos:prefLabel "micrometer"@en ;
    skos:altLabel "µm"@en ; 
    skos:definition "micrometer"@en ;
.

