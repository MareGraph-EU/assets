@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@prefix trait: <https://aphia.org/ns/vocab/attribute-trait/> .

trait:conceptscheme
    a skos:Conceptscheme ;
    rdfs:label "listing aphia attributes and traits"@en .

<https://aphia.org/ns/vocab/attribute-trait/>
    a skos:Collection ;
    rdfs:label "aphia attributes and traits"@en ;
    
    skos:member trait:vme_indicative_taxon ;
.

trait:vme_indicative_taxon
    a skos:Collection ; 
    dct:description "..."@en ;

    skos:member trait:vme_indicative_taxon_yes , trait:vme_indicative_taxon_no ;
. 

trait:vme_indicative_taxon_yes
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "Yes"@en ;
    skos:definition "..."@en ;
.

trait:vme_indicative_taxon_no
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "No"@en ;
    skos:definition ""@en ;
.