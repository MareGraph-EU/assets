@prefix dct: <http://purl.org/dc/terms/> .
@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@base <https://aphia.org/ns/traits/vme_indicative_taxon> .

<>
    a skos:Collection ; 
    dct:description "..."@en ;

    skos:member <#vme_indicative_taxon_yes> , <#vme_indicative_taxon_no> ;
. 

<#vme_indicative_taxon_yes>
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme <https://aphia.org/ns/traits/> ;
    skos:prefLabel "Yes"@en ;
    skos:definition "..."@en ;
.

<#vme_indicative_taxon_no>
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme <https://aphia.org/ns/traits/> ;
    skos:prefLabel "No"@en ;
    skos:definition ""@en ;
.