@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@prefix trait: <https://aphia.org/ns/vocab/attribute-trait/> .


trait:mobility
    a skos:Collection ; 
    dct:description "The general shape of the body of an organism."@en ;

    skos:member trait:mobile , trait:sessile , trait:sedentary ; 
.

trait:mobile
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "mobile"@en ;
    skos:definition "An organism or population that can change its location or distribution with time; mobility; vagility."@en ;

    skos:exactMatch ... ; 
.
trait:sessile
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "sessile"@en ;
    skos:definition "Non-motile; permanently attached at the base."@en ;

    skos:exactMatch ... ; 
.
trait:sedentary
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "sedentary"@en ;
    skos:definition "Attached to the substrate; not free-living."@en ;

    skos:exactMatch ... ; 
.