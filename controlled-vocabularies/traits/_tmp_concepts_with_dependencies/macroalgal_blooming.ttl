@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@base <https://aphia.org/ns/term/macroalgal_blooming> .

<>
    a skos:Collection ; 
    dct:description "Types of blooming specific for macroalgae species."@en ;

    skos:member <#macroalgal_blooming_yes> , <#macroalgal_blooming_no> ;

    skos:closeMatch <https://kos.lifewatch.eu/thesauri/traits/c_9854e4cf> ; 
. 

<#macroalgal_blooming_yes>
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme <https://aphia.org/ns/vocab/attribute-trait/> ;
    skos:prefLabel "Macroalgal blooming potential"@en ;
    skos:definition "The species has the potential to go through episodes of intense growth and mass proliferation of the thallus under specific environmental conditions such as high nutrient and temperature conditions."@en ;
.

<#macroalgal_blooming_no>
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme <https://aphia.org/ns/vocab/attribute-trait/> ;
    skos:prefLabel "No macroalgal blooming potential"@en ;
    skos:definition "The species does not have the obvious potential to go through episodes of intense growth and mass proliferation of the thallus under specific environmental conditions such as high nutrient and temperature conditions."@en ;
.