@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix aphia-trait: <https://aphia.org/ns/attribute-trait/> .

@prefix trait: <https://aphia.org/ns/vocab/attribute-trait/> .

trait:conceptScheme
    a skos:ConceptScheme ;
    rdfs:label "Listing Aphia attributes and traits"@en .

<https://aphia.org/ns/vocab/attribute-trait/>
    a skos:Collection ;
    rdfs:label "Aphia attributes and traits"@en ;
    
    skos:member trait:body_size_unit ;
.

trait:body_size_unit
    a skos:Collection ; 
    dct:description "The unit of a body size (e.g. m, cm, mm)."@en ;

    skos:member trait:micrometer , 
                trait:millimeter , 
                trait:centimeter , 
                trait:meter , 
                trait:square_centimeter , 
                trait:square_decimeter , 
                trait:square_meter , 
                trait:cubic_centimeter , 
                trait:inch , 
                trait:kilogram , 
                trait:ton ; 
.

trait:micrometer
    a skos:Concept, aphia-trait:Trait ;
    skos:inScheme trait:conceptScheme ;
    skos:prefLabel "micrometer"@en ;
    skos:altLabel "µm"@en ; 
    skos:definition "micrometer"@en ;
.
